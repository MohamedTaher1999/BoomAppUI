<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeFragment">


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/blue_rectangle"
            android:paddingBottom="37dp"
            >
            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/linearLayoutCompat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/marketContainer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rectangle_bg_green_a700_radius_8_5"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/_4pxh"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:src="@drawable/rectangle_bg_white_a700_radius_2" />

                    <TextView
                        android:id="@+id/marketText"
                        style="@style/txtIntermedium11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:text="@string/lbl_market_is_open"></TextView>

                </androidx.appcompat.widget.LinearLayoutCompat>


            </androidx.appcompat.widget.LinearLayoutCompat>

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginEnd="16dp"
                android:src="@drawable/img_notification"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="53dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/youtube_icon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />

            <LinearLayout
                android:id="@+id/linearRowgroup349"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_weight="0.66"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat">

                <FrameLayout
                    android:id="@+id/frameAvatar"
                    android:layout_width="@dimen/_48pxh"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/imageGroup349"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_gravity="top|center"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group349"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_group349" />

                    <LinearLayout
                        android:id="@+id/linearProBadge"
                        style="@style/groupStyleorange_700cornerRadius"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|start"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageSignal"
                            android:layout_width="@dimen/_10pxh"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_signal"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_signal" />

                        <TextView
                            android:id="@+id/txtPRO"
                            style="@style/txtIntersemibold12"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.7"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_pro" />
                    </LinearLayout>
                </FrameLayout>



                <TextView
                    android:id="@+id/txtAccCounter"
                    style="@style/txtIntermedium12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_weight="0.42"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textAllCaps="true"
                    android:text="@string/lbl_acc_01" />
                <ImageView
                    android:id="@+id/imageArrowright"
                    android:layout_width="@dimen/_8pxh"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowright" />
            </LinearLayout>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/linearLayoutCompat2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="50dp"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearRowgroup349">

                <TextView
                    android:id="@+id/txtDate"
                    style="@style/txtInterregular10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_04_aug_2023" />

                <TextView
                    android:id="@+id/txtHelloMark"
                    style="@style/txtIntermedium16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_hello_mark" />

            </androidx.appcompat.widget.LinearLayoutCompat>

            <LinearLayout
                android:id="@+id/linearTotalInvestmen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginTop="22dp"
                android:layout_weight="0.72"
                android:gravity="start"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat2">

                <LinearLayout
                    android:id="@+id/linearRowtotalinvestmen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/txtTotalInvestmen"
                        style="@style/txtInterregular12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_total_investmen" />
                    <ImageView
                        android:id="@+id/imageInfo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/information_circle"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

                <TextView
                    android:id="@+id/txtPrice"
                    style="@style/txtIntersemibold32"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_egp_225_000" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearProditLossC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="21dp"
                android:layout_marginTop="30dp"
                android:gravity="end"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat2">

                <LinearLayout
                    android:id="@+id/linearProfitLossC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtProfit"
                        style="@style/txtIntermedium10"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:ellipsize="end"
                        android:letterSpacing="0.1"
                        android:paddingTop="@dimen/_5pxv"
                        android:singleLine="true"
                        android:textAlignment="textEnd"
                        android:textAllCaps="true"
                        android:text="@string/lbl_profit" />
                    <ImageView
                        android:id="@+id/imageArrowdown"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowdown" />


                    <TextView
                        android:id="@+id/txtOneHundredTwentyFive"
                        style="@style/txtInterbold14"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_weight="0.43"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_12_5" />
                </LinearLayout>

                <TextView
                    android:id="@+id/txtPriceOne"
                    style="@style/txtInterregular14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textAlignment="center"
                    android:text="@string/lbl_egp_18_000" />
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <LinearLayout
            android:id="@+id/linearToggle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20.5dp"
            android:layout_marginStart="20.5dp"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            style="@style/groupStyleindigo_100cornerRadius">
            <TextView
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/btnMyProtfolio"
                android:layout_height="30dp"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="7dp"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight=".5"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_my_portfolio"
                android:textAllCaps="false"
                android:textSize="16dp"
                android:textColor="@color/indigo_900"
                android:fontFamily="@font/inter"
                android:textFontWeight="500"
                android:background="@drawable/rectangle_bg_grey_a700_radius_15_5"
                />
            <TextView
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/btnMyOrders"
                android:layout_height="30dp"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight=".5"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_my_orders"
                android:textAllCaps="false"
                android:textSize="16dp"
                android:textColor="@color/indigo_900"
                android:fontFamily="@font/inter"
                android:textFontWeight="500"
                android:background="@drawable/rectangle_bg_white_a700_radius_15_5"
                />
        </LinearLayout>
        <FrameLayout
            android:id="@+id/homeFragmentContainer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>